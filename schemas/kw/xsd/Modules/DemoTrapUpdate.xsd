			<xs:extension base="UpdateModuleData">
				<xs:sequence></xs:sequence>
		        <xs:attribute name="DetonationWeaponTemplate" type="WeaponRef" />  
		        <xs:attribute name="IgnoreKindOf" type="KindOfBitFlags" />  
		        <xs:attribute name="ManualModeWeaponSlot" type="WeaponSlotType" />  
		        <xs:attribute name="DetonationWeaponSlot" type="WeaponSlotType" />  
		        <xs:attribute name="ProximityModeWeaponSlot" type="WeaponSlotType" />  
		        <xs:attribute name="TriggerDetonationRange" type="SageReal" default="0" />  

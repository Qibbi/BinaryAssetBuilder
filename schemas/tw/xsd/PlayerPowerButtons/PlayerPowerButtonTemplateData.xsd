	<xs:include schemaLocation="../Includes/ButtonState.xsd"/>

	<!-- base class for unit ability button template data types -->
	<xs:complexType name="PlayerPowerButtonTemplateData" xas:isPolymorphic="true">
		<xs:sequence>
			<xs:element name="State" type="ButtonState" minOccurs="1" maxOccurs="1" />
		</xs:sequence>
		<xs:attribute name="id" type="SpecialPowerRef" use="required"/>
	</xs:complexType>

</xs:schema>

	<FXList id="FX_DisruptionTower_Die">
		<NuggetList>
			<!-- Play the "stealth going away" sound, but ONLY if the disruption tower is not already disabled -->
			<Sound 
				Value="NOD_DisruptionTower_Off" >
				<!-- It is necessary to do it this way, not just test model conditions, because if a structure is killed and 
				     that brings the power back online, the UNDERPOWERED model condition will have been erased by the time
						 we get here -->
				<SourceMustNotHaveBeenDisabledThisFrameByType>UNDERPOWERED</SourceMustNotHaveBeenDisabledThisFrameByType>
				<SourceMustNotHaveBeenDisabledThisFrameByType>EMP</SourceMustNotHaveBeenDisabledThisFrameByType>
			</Sound>
		</NuggetList>
	</FXList>
	
	<FXList id="FX_DisruptionTower_Created">
		<NuggetList>
			<!-- Play the "stealth coming own" sound, but ONLY if the disruption tower is not already underpowered -->
			<Sound 
				ExcludedSourceModelConditions="AFFECTED_BY_EMP UNDERPOWERED"
				Value="NOD_DisruptionTower_On" />
		</NuggetList>
	</FXList>

